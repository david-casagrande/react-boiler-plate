{
  "name": "react-boiler-plate",
  "version": "0.0.1",
  "description": "React setup with Webpack, Jest, and ESLint",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --content-base build",
    "build": "node_modules/.bin/webpack",
    "test": "node_modules/.bin/jest app",
    "lint": "if node_modules/.bin/eslint app; then exit 0; else exit 0; fi"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-jest": "^9.0.3",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "enzyme": "^2.2.0",
    "eslint": "2.2.0",
    "eslint-config-strict": "8.4.0",
    "eslint-plugin-filenames": "^0.2.0",
    "eslint-plugin-react": "^4.2.3",
    "jest-cli": "^0.9.2",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "jest": {
    "testPathDirs": [
      "<rootDir>"
    ],
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "enzyme"
    ],
    "globals": {},
    "verbose": true,
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
